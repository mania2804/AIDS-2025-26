
#include <iostream>
#include <cstdlib>
#include <iomanip>
using namespace std;


int main()
{
    
    int n;
    cin>>n;
    
    srand(time(0));
    
  
    int *tab=new int[n];
    
    
    
   
    for(int i=0; i<n; i++){
            int liczby=0;
            do {
                liczby=1000+rand()%1000;
                
            }
            
            while(liczby%3==0&&liczby%4==0);
            tab[i]=liczby;
            
    }
    
    
    
    for(int i=0; i<n; i++){
            cout<<setw(5)<<tab[i];
        }
    
    
  
     
    delete[] tab;
    
    
    return 0;
}
