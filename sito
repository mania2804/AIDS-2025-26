#include<iostream>
using namespace std;

int main()
{
    int N;
    cout<<"N: "; cin>>N;
    int *tab=new int[N];
    
    for(int i=1; i<=N; i++)
    {
        tab[i]=1;
    }
    
    for(int i=2; i<=N; i++ )
    {
        if(tab[i]!=0)
        {
    
        for(int j=2*i; j<=N; j+=i)
          {
            tab[j]=0;
          }
        }
    }
    
    cout<<"Liczby pierwsze do "<<N<<" to: ";
    for(int i=2; i<=N;i++)
    {
       if(tab[i]==1)
         cout<<i<<" ";
    }
    
    delete [] tab;
    
    return 0;
}
