#include <iostream>
#include <string>

struct Node{
    double wartosc;
    Node*next=nullptr;
};

void odloz(Node *&head, double x) {
    Node *nowy = new Node;
    nowy->wartosc = x;
    nowy->next = head;
    head=nowy;
}

void zdejmij(Node *&head) {
    if (head == nullptr) {
        cout << "Nie ma stosu" << endl;
        return;
    }

    Node*temp=head;
    head=head->next;
    delete temp;
}

void podejrzyj(Node *head, double wartosc) {
    if (head == nullptr) {
        cout << "Nie ma stosu." << endl;
        return;
    }

    Node *temp = head;
    for (int i = 1; i < n && temp != nullptr; i++) {
        temp = temp->next;
    }

    if (temp == nullptr) {
        cout << "Podany indeks jest za duzy." << endl;
    } else {
        cout << temp->wartosc<< endl;
    }
}

