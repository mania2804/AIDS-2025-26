#include <iostream>
#include <fstream>
#include <cctype>

using namespace std;

struct Node{
    int wartosc;
    char znak;
    Node * next= nullptr;

};
void odloz(Node *&head, double x) {
    Node *nowy = new Node;
    nowy->wartosc = x;
    nowy->next = head;
    head=nowy;
}

void zdejmij(Node *&head) {
    if (head == nullptr) {
        cout << "Nie ma stosu" << endl;
        return;
    }

    Node*temp=head;
    head=head->next;
    delete temp;
}


void oproznij(Node*&head){
    head=nullptr;
    
}

int main(){
    string wyrazenie="2+3";

    int n=wyrazenie.size;
    // char *tab=new char;
    // cout<<"Podaj wyraÅ¼enie:";
     for(int i=0; i<n; i++){
        if(isdigit(wyrazenie[i])){
            odloz(head, wartosc);
        }
        else
        odloz (head, znak);
     }
   

     







    return 0;
}

